function calcular() {
    const v1 =3D parseFloat(document.getElementById("valor1").value);
    const v2 =3D parseFloat(document.getElementById("valor2").value);
    const op =3D document.getElementById("operacao").value;
    let resultado;

    if (isNaN(v1) || isNaN(v2)) {
        alert("Por favor, insira n=C3=BAmeros v=C3=A1lidos.");
        return;
    }

    switch(op) {
        case '+':
            resultado =3D v1 + v2;
            break;
        case '-':
            resultado =3D v1 - v2;
            break;
        case '*':
            resultado =3D v1 * v2;
            break;
        case '/':
            if (v2 =3D=3D=3D 0) {
                alert("Divis=C3=A3o por zero n=C3=A3o =C3=A9 permitida.");
                return;
            }
            resultado =3D v1 / v2;
            break;
        default:
            alert("Opera=C3=A7=C3=A3o inv=C3=A1lida.");
            return;
    }

    alert("Resultado: " + resultado);
}

